#!/bin/bash

export PACKAGE_DOCKER_COMPOSE_VERSION_DEFAULT=v1.26.2

export PACKAGE_DOCKER_COMPOSE_BIN_DIR=/usr/local/bin
export PACKAGE_DOCKER_COMPOSE_BIN_NAME=${PACKAGE_DOCKER_COMPOSE_BIN_NAME:-"docker-compose"}
export PACKAGE_DOCKER_COMPOSE_BIN_PATH=$PACKAGE_DOCKER_COMPOSE_BIN_DIR/$PACKAGE_DOCKER_COMPOSE_BIN_NAME

export PACKAGE_DOCKER_COMPOSE_BIN_TMP_PATH=/tmp/docker-compose
